<mujoco model="walker2d">
    <compiler angle="degree" coordinate="global" inertiafromgeom="true" />
    <default>
        <joint armature="0.01" damping=".1" limited="true" />
        <geom conaffinity="0" condim="3" contype="1" density="1000" friction="0.8 .1 .1" rgba="0.8 0.6 .4 1" />
    </default>
    <option integrator="RK4" timestep="0.002" />
    <worldbody>
        <body name="torso">
            <geom fromto="0 0 $z0 0 0 $z1" name="torso_geom" size="$w0" type="capsule" />
            <body name="thigh">
                <joint axis="0 -1 0" name="thigh_joint" pos="0 0 $z1" range="-60 60" type="hinge" />
                <geom fromto="0 0 $z1 0 0 $z2" name="thigh_geom" size="$w1" type="capsule" />
                <body name="leg">
                    <joint axis="0 -1 0" name="leg_joint" pos="0 0 $z2" range="-150 0" type="hinge" />
                    <geom fromto="0 0 $z2 0 0 $z3" name="leg_geom" size="$w2" type="capsule" />
                    <body name="foot">
                        <joint axis="0 -1 0" name="foot_joint" pos="0 0 $z3" range="-45 45" type="hinge" />
                        <geom fromto="0 0 $z3 $x3 0 $z3" name="foot_geom" size="$w3" type="capsule" />
                    </body>
                </body>
            </body>
            <body name="thigh_left">
                <joint axis="0 -1 0" name="thigh_left_joint" pos="0 0 $z1" range="-60 60" type="hinge" />
                <geom fromto="0 0 $z1 0 0 $z2" name="thigh_left_geom" rgba=".7 .3 .6 1" size="$w1" type="capsule" />
                <body name="leg_left">
                    <joint axis="0 -1 0" name="leg_left_joint" pos="0 0 $z2" range="-150 0" type="hinge" />
                    <geom fromto="0 0 $z2 0 0 $z3" name="leg_left_geom" rgba=".7 .3 .6 1" size="$w2" type="capsule" />
                    <body name="foot_left">
                        <joint axis="0 -1 0" name="foot_left_joint" pos="0 0 $z3" range="-45 45" type="hinge" />
                        <geom fromto="0 0 $z3 $x3 0 $z3" name="foot_left_geom" rgba=".7 .3 .6 1" size="$w3" type="capsule" />
                    </body>
                </body>
            </body>
        </body>
    </worldbody>
    <actuator>
        <motor ctrllimited="true" ctrlrange="-1.0 1.0" gear="100" joint="thigh_joint" />
        <motor ctrllimited="true" ctrlrange="-1.0 1.0" gear="100" joint="leg_joint" />
        <motor ctrllimited="true" ctrlrange="-1.0 1.0" gear="100" joint="foot_joint" />
        <motor ctrllimited="true" ctrlrange="-1.0 1.0" gear="100" joint="thigh_left_joint" />
        <motor ctrllimited="true" ctrlrange="-1.0 1.0" gear="100" joint="leg_left_joint" />
        <motor ctrllimited="true" ctrlrange="-1.0 1.0" gear="100" joint="foot_left_joint" />
    </actuator>
</mujoco>
