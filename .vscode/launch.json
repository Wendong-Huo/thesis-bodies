    "version": "0.2.0",
    "configurations": [
        {
            "name": "cleaned single _train.py",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/cleaned_version/_train.py",
            "cwd": "${workspaceFolder}/cleaned_version",
            "args": [
                "--exp-name=walker2d_20_10-v0",
                "--exp-idx=19",
                "--single",
                "--single-idx=19",
                "--seed=1",
                // "--with-bodyinfo",
                "--dataset=dataset/walker2d_20_10-v0"
            ],
            "console": "integratedTerminal",
            "justMyCode": false,
        },
        {
            "name": "cleaned multi _train.py",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/cleaned_version/_train.py",
            "cwd": "${workspaceFolder}/cleaned_version",
            "args": [
                "--exp-name=walker2d_20_10-v0",
                "--exp-idx=0",
                "--body-ids=18,1,19,8,10,17,6,13,4,2,5,14,9,7,16,11",
                "--seed=1",
                "--with-bodyinfo",
                "--dataset=dataset/walker2d_20_10-v0",
                "--n-timesteps=10000000.0"
            ],
            "console": "integratedTerminal",
            "justMyCode": false,
        },
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "cwd": "${fileDirname}",
            "console": "integratedTerminal",
            "justMyCode": false,
        },
        {
            "name": "Enjoy",
            "type": "python",
            "request": "launch",
            "program": "enjoy.py",
            "args": [
                // "--dataset=dataset/walker_toy_v5",
                "-f=logs/75/ppo/Walker2Ds-v0_1/",
                "--load-checkpoint=logs/75/ppo/Walker2Ds-v0_1/best_model.zip",
                "--body-id=75",
                "--dataset=dataset/walker2d_v6",
                "--deterministic"
            ],
            "console": "integratedTerminal",
            "justMyCode": false,
        },
        {
            "name": "Train",
            "type": "python",
            "request": "launch",
            "program": "train.py",
            "args": [
                "--watch-train",
                "-tb=tb/train_on_10",
                "-f=logs/train_on_10",
                "--algo=ppo1",
                "--dataset=dataset/walker2d_v6",
                "--seed=0",
                // "--single-idx=9",
                "--single-group=0",
            ],
            "console": "integratedTerminal",
            "justMyCode": false,
        }
    ]
}